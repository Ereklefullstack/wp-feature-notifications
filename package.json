{
	"name": "@wordpress/wp-feature-notifications",
	"version": "0.1.0",
	"description": "A notification center for WordPress.",
	"keywords": [
		"WordPress",
		"notifications",
		"dashboard"
	],
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"bugs": {
		"url": "https://github.com/WordPress/wp-feature-notifications/issues"
	},
	"homepage": "https://github.com/WordPress/wp-feature-notifications#readme",
	"directories": {
		"doc": "docs",
		"test": "tests"
	},
	"files": [
		"build/*",
		"includes/*",
		"readme.txt",
		"wp-feature-notifications.php"
	],
	"scripts": {
		"start": "wp-scripts start",
		"build": "wp-scripts build",
		"prephp-install": "wp-env start",
		"php-install": "wp-env run composer 'composer --ignore-platform-req=php install'",
		"pretest-unit-php": "npm run php-install",
		"test-unit-php": "wp-env run phpunit 'phpunit --configuration=/var/www/html/wp-content/plugins/wp-notify/phpunit.xml.dist'",
		"packages-update": "wp-scripts packages-update",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"plugin-zip": "wp-scripts plugin-zip",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js",
		"storybook": "start-storybook -p 6006",
		"build-storybook": "build-storybook",
		"docGen": "npx docgen src/scripts/wp-notify.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/WordPress/wp-feature-notifications.git"
	},
	"engines": {
		"node": ">=12.13",
		"npm": ">=6.9"
	},
	"devDependencies": {
		"@babel/core": "^7.20.7",
		"@reduxjs/toolkit": "^1.9.1",
		"@storybook/addon-actions": "^6.5.15",
		"@storybook/addon-essentials": "^6.5.15",
		"@storybook/addon-interactions": "^6.5.15",
		"@storybook/addon-links": "^6.5.15",
		"@storybook/builder-webpack5": "^6.5.15",
		"@storybook/manager-webpack5": "^6.5.15",
		"@storybook/react": "^6.5.15",
		"@storybook/testing-library": "^0.0.13",
		"@wordpress/api-fetch": "^6.20.0",
		"@wordpress/babel-preset-default": "^7.7.0",
		"@wordpress/components": "^23.0.0",
		"@wordpress/docgen": "^1.32.0",
		"@wordpress/element": "^5.0.0",
		"@wordpress/i18n": "^4.23.0",
		"@wordpress/icons": "^9.14.0",
		"@wordpress/scripts": "^25.0.0",
		"babel-loader": "^9.1.0",
		"browserslist": "^4.20.3",
		"classnames": "^2.3.2",
		"moment": "^2.29.3",
		"prop-types": "^15.8.1"
	},
	"dependencies": {
		"dompurify": "^2.4.0"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"babel": {
		"presets": [
			"@wordpress/babel-preset-default"
		]
	}
}
